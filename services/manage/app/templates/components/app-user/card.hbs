<ListCard>
  <div class="card-body">
    <h2 class="card-title">{{this.user.givenName}} {{this.user.familyName}}</h2>
    <p class="card-text">{{this.user.email}}</p>
  </div>
  <div class="card-footer">
    {{#link-to "manage.orgs.view.apps.view.users.edit" this.user.id class="btn btn-primary" role="button"}}
      {{entypo-icon "edit" class="mr-1"}} Modify
    {{/link-to}}
  </div>
</ListCard>
